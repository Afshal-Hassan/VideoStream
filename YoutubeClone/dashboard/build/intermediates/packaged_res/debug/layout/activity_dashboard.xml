<?xml version="1.0" encoding="utf-8"?>


                                                                  
                                                      
                                                            


    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical"
            android:paddingTop="5dp"
            tools:context=".screens.Dashboard" android:tag="layout/activity_dashboard_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto">


        <fragment
                android:id="@+id/header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:name="com.youtube.common_utils.Header"
                tools:layout="@layout/fragment_header"/>


        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


            <fragment
                    android:id="@+id/videoFragment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:name="com.youtube.dashboard.fragments.Video"
                    tools:layout="@layout/fragment_video"/>


        </LinearLayout>


    </LinearLayout>

         